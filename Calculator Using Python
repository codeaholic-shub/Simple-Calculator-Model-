import sys
print(''' Please Select Operation:
1) Add
2) Subtract
3) Multiply
''')
choice=int(input('Enter Your Operation:'))
if choice not in(1,2,3):
    print('Invalid Choice !')
    sys.exit()
a=int(input())
b=int(input())
if choice==1:
    res=a+b
elif choice==2:
    res=a-b
else:
    res=a*b

print("Result is:",res)
